  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "^2.232.1-alpha.0",
    "aws-cdk-lib": "^2.232.1",
    "constructs": "^10.4.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
