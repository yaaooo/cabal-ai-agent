{
  "name": "@cabal-ai-agent/cabal-infra",
  "version": "1.0.0",
  "description": "Infrastructure package for the CABAL AI agent. Deploys AWS resources with CDK.",
  "license": "MIT",
  "author": "Yao-Hui Chua <hello@yao.page> (https://yao.page)",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
